+import ShowComment from "./showComment";
+
+const CommentDetails = ({item,socket}) => {
+    const accountId = JSON.parse(localStorage.getItem("accountId"))
+    return (
+        <div>
+            {
+                item.map((comment)=>{
+
+                    if (comment.accountId !== accountId){
+                        return(
+                            <ShowComment comment={comment}  item={item} socket={socket} isDelete={false}/>
+                        )
+                    } else {
+                        return(
+                            <ShowComment comment={comment} item={item} socket={socket}  isDelete={true}/>
+                        )
+                    }
+                })
+            }
+
+        </div>
+    )
+}
